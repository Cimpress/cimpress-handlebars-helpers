{
  "name": "cimpress-handlebars-helpers",
  "version": "0.0.4",
  "description": "A set of simple handlebars helpers",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --compilers js:babel-core/register tests/",
    "code-check": "eslint --ext .js --ext .jsx src",
    "cover": "node ./node_modules/nyc/bin/nyc.js ./node_modules/mocha/bin/mocha --require babel-core/register tests/",
    "build": "node ./node_modules/babel-cli/bin/babel.js src -d lib --copy-files"
  },
  "author": "TrdelnikSquad@cimpress.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cimpress/cimpress-handlebars-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/Cimpress/cimpress-handlebars-helpers/issues"
  },
  "homepage": "https://github.com/Cimpress/cimpress-handlebars-helpers#readme",
  "keywords": [
    "cimpress",
    "handlebars",
    "helpers"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-config-google": "^0.9.1",
    "handlebars": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "lodash.flatmap": "^4.5.0",
    "lodash.get": "^4.4.2"
  }
}
